@import 'nib';
@import "./public/stylesheets/vars";
web-chooser
  display block;
  width 100%;
  height 100%;
  & > #wrapper
    height 50px;
    width 100%;
    web-ripple-button
      display inline-block;
    position relative;
    & > #text
      height 50px;
      line-height 48px;
      font-size 18px;
      color swampTextColor;
      margin-left 50px;
    #icons
      display inline-block;
      width 50px;
      height 50px;
      position absolute;
      z-index 4;
      cursor pointer;
      top:0;
      left: 0;
